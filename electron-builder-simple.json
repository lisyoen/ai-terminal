{
  "appId": "com.lisyoen.ai-terminal",
  "productName": "AI Terminal",
  "directories": {
    "output": "dist/release",
    "buildResources": "build"
  },
  "files": [
    "dist/**",
    "package.json",
    "!**/*.map",
    "!**/node_modules/.cache/**",
    "!src/**",
    "!electron/**",
    "!types/**",
    "!.env",
    "!.env.*",
    "!*.config.*",
    "!tsconfig.json"
  ],
  "win": {
    "target": "dir"
  },
  "forceCodeSigning": false,
  "compression": "normal"
}